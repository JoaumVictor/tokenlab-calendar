generator client {
  provider = "prisma-client-js"
  output   = "./src/generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int     @id @default(autoincrement())
  username String
  email    String
  password String
  events   Event[]
}

model Event {
  id          Int      @id @default(autoincrement())
  description String
  startTime   DateTime
  endTime     DateTime
  userId      Int
  user        User     @relation(fields: [userId], references: [id])
}
