// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Event {
  start       DateTime
  title       String
  allDay      Boolean
  id          Int       @id @default(autoincrement())
  description String
  userId      Int
  end         DateTime?
}
